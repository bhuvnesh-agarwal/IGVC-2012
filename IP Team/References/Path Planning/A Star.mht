.social-share-wrapper{
	margin: 6px 20px;
	
}

.socialMedia-toolbar{
	background: #fff url("/images/global/neutral/sbs/socialToolbar-bg.png") 0 bottom repeat-x;
	height: 20px;
	width: 152px;
	display: block;
	float: right;
	border: 1px solid #738CB6;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	*display: block;
	position: relative;
}

/* Icons Sprited*/
.at300bs{
	background: url("/images/global/neutral/sbs/socialToolbar-icon-sprite.png") no-repeat;
	width: 15px;
}		
.at300bs.at15t_compact{background-position: 0px 4px; display: block; width:62px;margin: 0 4px;}
.at300bs.at15t_facebook{background-position: -2px -22px;}
.at300bs.at15t_twitter{background-position: 0px -47px;}
.at300bs.at15t_linkedin{background-position: -2px -73px;}

#at15s_head{
	background: url("/images/global/neutral/sbs/socialToolbar-bg-popup.png") 0 0 repeat-x;
	border-bottom: 1px solid  #989898;
	height: 18px;
	line-height: 18px;	
}

#at16pf{
	height: 23px;
	line-height: 23px;
	background: #F1F1F1;
	border-top: 1px solid #fff;
}

#at15s_inner{
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
}

a#at15sptx{
	background: url("/images/global/neutral/sbs/socialToolbar-bg-popup-close.png") 0 0 no-repeat;
	width: 16px;
	height: 16px;
	overflow: hidden;
	text-indent: -10000px;
}

#at16pf a#at-logo{
	background: url("/images/global/neutral/sbs/socialToolbar-bg-popup-logo.png") 0 7px no-repeat;
	padding-left: 13px;
}

.addthis_default_style .at300b, .addthis_default_style .at300m{
	border-right: 1px solid #738cb6;
	height: 20px;
	margin: 0  2px;
}

.addthis_button_linkedin{
	border-right: none!important;
}
